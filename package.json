{
  "name": "blog-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate:prod": "dotenv -e .env.production -- prisma generate",
    "db:generate:local": "dotenv -e .env.local -- prisma generate",
    "db:push:prod": "dotenv -e .env.production -- prisma db push",
    "db:push:local": "dotenv -e .env.local -- prisma db push",
    "db:migrate:prod": "dotenv -e .env.production -- prisma migrate prod",
    "db:migrate:local": "dotenv -e .env.local -- prisma migrate dev",
    "db:studio:prod": "dotenv -e .env.production -- prisma studio",
    "db:studio:local": "dotenv -e .env.local -- prisma studio",
    "db:seed:prod": "dotenv -e .env.production -- tsx prisma/seed.ts",
    "db:seed:local": "dotenv -e .env.local -- tsx prisma/seed.ts",
    "dev:local": "dotenv -e .env.local -- next dev --turbopack",
    "build:prod": "dotenv -e .env.production -- next build"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.4.5",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@types/mdx": "^2.0.13",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fumadocs-mdx": "^11.7.3",
    "fumadocs-ui": "^15.6.9",
    "lucide-react": "^0.536.0",
    "mysql2": "^3.14.3",
    "next": "15.4.5",
    "next-auth": "^4.24.11",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.6",
    "prisma": "^6.13.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.14",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
}
